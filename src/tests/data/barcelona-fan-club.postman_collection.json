{
    "info": {
        "_postman_id": "random-uuid",
        "name": "Barcelona Fan Club API",
        "description": "Collection for testing Barcelona Fan Club API endpoints",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Authentication",
            "item": [
                {
                    "name": "Register",
                    "request": {
                        "method": "POST",
                        "header": [],
                        "body": {
                            "mode": "formdata",
                            "formdata": [
                                {
                                    "key": "email",
                                    "value": "test@example.com",
                                    "type": "text"
                                },
                                {
                                    "key": "password",
                                    "value": "Password123!",
                                    "type": "text"
                                },
                                {
                                    "key": "dateOfBirth",
                                    "value": "1990-01-01",
                                    "type": "text"
                                },
                                {
                                    "key": "gender",
                                    "value": "male",
                                    "type": "text"
                                },
                                {
                                    "key": "address",
                                    "value": "Jl. Testing No. 123, Jakarta",
                                    "type": "text"
                                },
                                {
                                    "key": "facePhoto",
                                    "type": "file",
                                    "src": "/path/to/photo.jpg"
                                }
                            ]
                        },
                        "url": {
                            "raw": "http://localhost:3000/auth/register",
                            "protocol": "http",
                            "host": [
                                "localhost"
                            ],
                            "port": "3000",
                            "path": [
                                "auth",
                                "register"
                            ]
                        },
                        "description": "Register new user with form-data for file upload"
                    },
                    "response": []
                },
                {
                    "name": "Login",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n    \"email\": \"test@example.com\",\n    \"password\": \"Password123!\"\n}"
                        },
                        "url": {
                            "raw": "http://localhost:3000/auth/login",
                            "protocol": "http",
                            "host": [
                                "localhost"
                            ],
                            "port": "3000",
                            "path": [
                                "auth",
                                "login"
                            ]
                        },
                        "description": "Login with email and password"
                    },
                    "response": []
                }
            ]
        },
        {
            "name": "News",
            "item": [
                {
                    "name": "Get Regular News",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}",
                                "type": "text"
                            }
                        ],
                        "url": {
                            "raw": "http://localhost:3000/news/regular",
                            "protocol": "http",
                            "host": [
                                "localhost"
                            ],
                            "port": "3000",
                            "path": [
                                "news",
                                "regular"
                            ]
                        },
                        "description": "Get regular news (requires authentication)"
                    },
                    "response": []
                },
                {
                    "name": "Get Exclusive News",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}",
                                "type": "text"
                            }
                        ],
                        "url": {
                            "raw": "http://localhost:3000/news/exclusive",
                            "protocol": "http",
                            "host": [
                                "localhost"
                            ],
                            "port": "3000",
                            "path": [
                                "news",
                                "exclusive"
                            ]
                        },
                        "description": "Get exclusive news (requires authentication and premium membership)"
                    },
                    "response": []
                }
            ]
        },
        {
            "name": "Membership",
            "item": [
                {
                    "name": "Get Membership Card",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}",
                                "type": "text"
                            }
                        ],
                        "url": {
                            "raw": "http://localhost:3000/membership/card",
                            "protocol": "http",
                            "host": [
                                "localhost"
                            ],
                            "port": "3000",
                            "path": [
                                "membership",
                                "card"
                            ]
                        },
                        "description": "Get user's membership card details"
                    },
                    "response": []
                },
                {
                    "name": "Register Membership",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}",
                                "type": "text"
                            }
                        ],
                        "body": {
                            "mode": "formdata",
                            "formdata": [
                                {
                                    "key": "identityCard",
                                    "type": "file",
                                    "src": "/path/to/identity-card.jpg"
                                }
                            ]
                        },
                        "url": {
                            "raw": "{{base_url}}/membership/register",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "membership",
                                "register"
                            ]
                        },
                        "description": "Register for membership by uploading identity card"
                    },
                    "response": []
                }
            ]
        },
        {
            "name": "Profile",
            "item": [
                {
                    "name": "Get Profile",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}",
                                "type": "text"
                            }
                        ],
                        "url": {
                            "raw": "http://localhost:3000/profile",
                            "protocol": "http",
                            "host": [
                                "localhost"
                            ],
                            "port": "3000",
                            "path": [
                                "profile"
                            ]
                        },
                        "description": "Get user profile information"
                    },
                    "response": []
                },
                {
                    "name": "Update Profile",
                    "request": {
                        "method": "PUT",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{token}}",
                                "type": "text"
                            }
                        ],
                        "body": {
                            "mode": "formdata",
                            "formdata": [
                                {
                                    "key": "email",
                                    "value": "updated@example.com",
                                    "type": "text"
                                },
                                {
                                    "key": "dateOfBirth",
                                    "value": "1990-01-01",
                                    "type": "text"
                                },
                                {
                                    "key": "gender",
                                    "value": "male",
                                    "type": "text"
                                },
                                {
                                    "key": "address",
                                    "value": "Updated Address",
                                    "type": "text"
                                },
                                {
                                    "key": "facePhoto",
                                    "type": "file",
                                    "src": "/path/to/new-photo.jpg"
                                }
                            ]
                        },
                        "url": {
                            "raw": "http://localhost:3000/profile",
                            "protocol": "http",
                            "host": [
                                "localhost"
                            ],
                            "port": "3000",
                            "path": [
                                "profile"
                            ]
                        },
                        "description": "Update user profile information"
                    },
                    "response": []
                }
            ]
        }
    ],
    "auth": {
        "type": "bearer",
        "bearer": [
            {
                "key": "token",
                "value": "{{token}}",
                "type": "string"
            }
        ]
    },
    "event": [
        {
            "listen": "prerequest",
            "script": {
                "type": "text/javascript",
                "exec": [
                    ""
                ]
            }
        },
        {
            "listen": "test",
            "script": {
                "type": "text/javascript",
                "exec": [
                    ""
                ]
            }
        }
    ],
    "variable": [
        {
            "key": "token",
            "value": "",
            "type": "string"
        }
    ]
}